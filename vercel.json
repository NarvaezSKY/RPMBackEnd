{
    "functions": {
      "api/**/*.js": {
        "runtime": "nodejs16"
      }
    },
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/$1" }
    ],
    "builds": [
      { "src": "api/**/*.js", "use": "@vercel/node" },
      { "src": "!(api)**/*.js", "use": "@vercel/static" }
    ]
  }
  